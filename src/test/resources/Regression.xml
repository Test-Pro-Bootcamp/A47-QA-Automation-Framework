<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Regression" parallel="methods" thread-count="3">
    <parameter name="baseURL" value="https://qa.koel.app/" />
    <parameter name="Playlist" value="Beta" />
    <parameter name="NewPlaylistName" value="Alpha" />
    <test name="Example Test" preserve-order="false">
        <classes>
            <class name="LoginTest">
                <methods>
                    <include name="correctLoginTests" />
                    <include name="incorrectLoginTests" />
                </methods>
            </class>
            <class name="SongsTest"/>
            <class name="PlayControlsTest"/>
            <class name="RegistrationTest"/>
        </classes>
    </test>
</suite>